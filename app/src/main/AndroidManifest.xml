<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sneha.smartagro">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sneha.smartagro.GovWebsite"></activity>
        <activity android:name="com.sneha.smartagro.GovernmentWebsites" />
        <activity android:name="com.sneha.smartagro.Apriori" />
        <activity android:name="com.sneha.smartagro.Analysis" />
        <activity android:name="com.sneha.smartagro.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name="com.sneha.smartagro.Regression" />
        <activity android:name="com.sneha.smartagro.Production" />
        <activity android:name="com.sneha.smartagro.Home1" />
        <activity android:name="com.sneha.smartagro.Schemes" />
        <activity android:name="com.sneha.smartagro.Treatment" />
        <activity android:name="com.sneha.smartagro.TreatmentDetail" />
        <activity android:name="com.sneha.smartagro.CropDetailActivity" />
        <activity android:name="com.sneha.smartagro.CropActivity" />
        <activity android:name="com.sneha.smartagro.HorticultureActivity" />
        <activity android:name="com.sneha.smartagro.HorticultureDetailActivity" />
    </application>

</manifest>